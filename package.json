{
  "name": "webpack-log",
  "version": "2.0.0",
  "description": "A common logger for the webpack ecosystem",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "lint": "npm run lint:js && npm run lint:prettier",
