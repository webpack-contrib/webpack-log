{
  "name": "webpack-log",
  "version": "1.2.0",
  "description": "A common logger for the webpack ecosystem",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "beautify": "npm run lint -- --fix",
    "ci": "npm run cover -- --report lcovonly && npm run test",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha",
    "lint": "eslint index.js test",
    "mocha": "mocha --full-trace --check-leaks",
    "test": "npm run lint && npm run mocha",
    "release": "standard-version"
  },
  "dependencies": {
    "ansi-colors": "^3.0.0",
    "loglevelnext": "^1.0.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "codecov.io": "^0.1.6",
    "eslint": "^4.5.0",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-import": "^2.7.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.0",
    "sinon": "^4.0.1",
    "standard-version": "^4.4.0",
    "strip-ansi": "^4.0.0"
  },
  "author": "Andrew Powell <andrew@shellscape.org>",
  "issues": "https://github.com/webpack-contrib/webpack-log/issues",
  "homepage": "https://github.com/webpack-contrib/webpack-log#readme",
  "repository": "https://github.com/webpack-contrib/webpack-log.git",
  "license": "MIT"
}
